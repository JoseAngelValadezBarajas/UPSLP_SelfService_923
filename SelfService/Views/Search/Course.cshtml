@section Styles{
    <link rel="stylesheet" href="~/css/Course.css" asp-append-version="true" />
}
<div id="root"></div>
<input id="hdnClassLevel" type="hidden" value="@ViewBag.ClassLevel" />
<input id="hdnCollege" type="hidden" value="@ViewBag.College" />
<input id="hdnCourseCode" type="hidden" value="@ViewBag.CourseCode" />
<input id="hdnCreditType" type="hidden" value="@ViewBag.CreditType" />
<input id="hdnCurriculum" type="hidden" value="@ViewBag.Curriculum" />
<input id="hdnDepartment" type="hidden" value="@ViewBag.Department" />
<input id="hdnEventSubType" type="hidden" value="@ViewBag.EventSubType" />
<input id="hdnKeywords" type="hidden" value="@ViewBag.Keywords" />
<input id="hdnNonTradProgram" type="hidden" value="@ViewBag.NonTradProgram" />
<input id="hdnPopulation" type="hidden" value="@ViewBag.Population" />
<input id="hdnProgram" type="hidden" value="@ViewBag.Program" />
@section Scripts{
    <script src="~/js/Course.js" asp-append-version="true"></script>
    <script src="~/js/SearchVendors.js" asp-append-version="true"></script>
}